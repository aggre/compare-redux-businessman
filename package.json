{
  "name": "compare-redux-businessman",
  "version": "1.0.0",
  "private": true,
  "description": "Compare Redux vs Businessman",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "async": "file://businessman"
  },
  "devDependencies": {
    "eslint-config-xo-react": "^0.10.0",
    "eslint-plugin-react": "^6.8.0",
    "xo": "^0.17.1"
  },
  "xo": {
    "envs": [
      "worker",
      "es6",
      "node",
      "browser"
    ],
    "rules": {
      "unicorn/filename-case": [
        0
      ],
      "import/prefer-default-export": [
        0
      ],
      "array-bracket-spacing": [
        2,
        "always"
      ],
      "object-curly-spacing": [
        2,
        "always"
      ],
      "computed-property-spacing": [
        2,
        "always"
      ],
      "space-before-function-paren": [
        2,
        "always"
      ],
      "space-in-parens": [
        2,
        "always"
      ],
      "space-before-blocks": 2
    },
    "overrides": [
      {
        "files": "test/client/*.js",
        "globals": [
          "describe",
          "it",
          "expect",
          "before",
          "beforeEach"
        ]
      }
    ],
    "extends": "xo-react",
    "semicolon": false
  }
}
